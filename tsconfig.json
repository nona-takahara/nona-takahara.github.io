{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@data/*": [
        "src/data/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@app-icon/*": [
        "src/data/applist/icons/*"
      ],
      "@/*": [
        "*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
